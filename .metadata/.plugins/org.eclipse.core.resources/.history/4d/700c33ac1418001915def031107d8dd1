package com.selenuim.basic;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.chrome.ChromeOptions;

public class OptionsClassSelenium {
	
	//Chrome Set Binary Function
	System.setProperty("webdriver.chrome.driver", "D:\\PATH\\chromedriver.exe");
	ChromeOptions co = new ChromeOptions();
	// set the binary path
	co.setBinary("C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe");
	//open the browser
	WebDriver driver = new ChromeDriver(co);
	driver.get("https://google.com");


}
